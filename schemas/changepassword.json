{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Change Password",
    "properties": {
        "key": {
            "type": "string",
            "description": "Key to use for encryption/decryption"
        },
        "action": {
            "type": "integer",
            "description": "Integer for action to take."
        },
        "label": {
            "type": "string",
            "description": "Label identifier of password to change."
        },
        "value": {
            "type": "string",
            "description": "The value for the new password to set for the label."
        }
    },
    "required": ["key", "action", "label", "value"]
}
