{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Get Password",
    "properties": {
        "key": {
            "type": "string",
            "description": "Key to use for encryption/decryption"
        },
        "action": {
            "type": "integer",
            "description": "Integer for action to take."
        },
        "label": {
            "type": "string",
            "description": "Label identifier of password to get."
        }
    },
    "required": ["key", "action", "label"]
}
